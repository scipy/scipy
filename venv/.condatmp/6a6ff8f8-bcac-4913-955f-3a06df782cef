prefix=/workspaces/scipy/venv
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

KDB5_DB_LIB=

Name: kdb
Description: Kerberos database access libraries
Version: 1.20.1
Requires.private: mit-krb5-gssapi mit-krb5 gssrpc
Cflags: -I${includedir}
Libs: -L${libdir} -lkdb5
Libs.private: ${KDB5_DB_LIB}
