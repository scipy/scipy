partial alphanumeric_keys
xkb_symbols "afnor" {

    name[Group1]="French (AZERTY, AFNOR)";

    key <TLDE>	{[               at,     numbersign,        dead_breve, dead_invertedbreve ]}; // @ #  Ì‘  Ì†
    key <AE01>	{[           agrave,              1,           section,             Agrave ]}; // Ã  1 Â§ Ã€
    key <AE02>	{[           eacute,              2,        dead_acute,             Eacute ]}; // Ã© 2  Ì Ã‰
    key <AE03>	{[           egrave,              3,        dead_grave,             Egrave ]}; // Ã¨ 3  Ì€ Ãˆ
    key <AE04>	{[      ecircumflex,              4,         ampersand,        Ecircumflex ]}; // Ãª 4 & ÃŠ
    key <AE05>	{[        parenleft,              5,       bracketleft,   dead_doubleacute ]}; // ( 5 [  Ì‹
    key <AE06>	{[       parenright,              6,      bracketright,   dead_doublegrave ]}; // ) 6 ]  Ì
    key <AE07>	{[  leftsinglequotemark,          7,       dead_macron,               none ]}; // â€˜ 7  Ì„
    key <AE08>	{[ rightsinglequotemark,          8,        underscore,             emdash ]}; // â€™ 8 _ â€”
    key <AE09>	{[    guillemotleft,              9,   leftdoublequotemark,          U2039 ]}; // Â« 9 â€œ â€¹
    key <AE10>	{[   guillemotright,              0,  rightdoublequotemark,          U203A ]}; // Â» 0 â€ â€º
    key <AE11>	{[       apostrophe,       quotedbl,            degree,     dead_abovering ]}; // ' " Â°  ÌŠ
    key <AE12>	{[  dead_circumflex, dead_diaeresis,        dead_caron,               none ]}; //  Ì‚  Ìˆ  ÌŒ

    key <AD01>	{[                a,              A,                ae,                 AE ]}; // a A Ã¦ Ã†
    key <AD02>	{[                z,              Z,          sterling,               none ]}; // z Z Â£
    key <AD03>	{[                e,              E,          EuroSign,               none ]}; // e E â‚¬
    key <AD04>	{[                r,              R,        registered,               none ]}; // r R Â®
    key <AD05>	{[                t,              T,         braceleft,          trademark ]}; // t T { â„¢
    key <AD06>	{[                y,              Y,        braceright,               none ]}; // y Y }
    key <AD07>	{[                u,              U,            ugrave,             Ugrave ]}; // u U Ã¹ Ã™
    key <AD08>	{[                i,              I,     dead_abovedot,      dead_belowdot ]}; // i I  Ì‡  Ì£
    key <AD09>	{[                o,              O,                oe,                 OE ]}; // o O Å“ Å’
    key <AD10>	{[                p,              P,           percent,           permille ]}; // p P % â€°
    key <AD11>	{[            minus,         endash,             U2212,              U2011 ]}; // - â€“ âˆ’ â€‘ // signe moins (minus sign), trait d'union insÃ©cable (non-breaking hyphen)
    key <AD12>	{[             plus,      plusminus,            dagger,       doubledagger ]}; // + Â± â€  â€¡

    key <AC01>	{[                q,              Q,       Greek_theta,               none ]}; // q Q Î¸
    key <AC02>	{[                s,              S,            ssharp,              U1E9E ]}; // s S ÃŸ áº // lettre majuscule latine S dur (latin capital letter sharp s)
    key <AC03>	{[                d,              D,            dollar,               none ]}; // d D $
    key <AC04>	{[                f,              F,     dead_currency,               none ]}; // f F Â¤
    key <AC05>	{[                g,              G,        dead_greek,               none ]}; // g G Âµ
    key <AC06>	{[                h,              H,              none,   dead_belowmacron ]}; // h H    Ì± // Missing deadkey for other european characters (ÂªÉ™ÆÃ¾ÃÄ±Ä°ÂºÅ¿Ã°ÃÆÈ Ä³Ä²)
    key <AC07>	{[                j,              J,              none,               none ]}; // j J
    key <AC08>	{[                k,              K,       dead_stroke,               none ]}; // k K  Ì·
    key <AC09>	{[                l,              L,               bar,               none ]}; // l L |
    key <AC10>	{[                m,              M,          infinity,               none ]}; // m M âˆ
    key <AC11>	{[            slash,      backslash,          division,            radical ]}; // / \ Ã· âˆš
    key <BKSL>	{[         asterisk,        onehalf,          multiply,         onequarter ]}; // * Â½ Ã— Â¼

    key <LSGT>	{[             less,        greater,     lessthanequal,  greaterthanequal ]}; // < > â‰¤ â‰¥
    key <AB01>	{[                w,              W,               ezh,               EZH ]}; // w W Ê’ Æ·
    key <AB02>	{[                x,              X,         copyright,              none ]}; // x X Â©
    key <AB03>	{[                c,              C,          ccedilla,          Ccedilla ]}; // c C Ã§ Ã‡
    key <AB04>	{[                v,              V,      dead_cedilla,       dead_ogonek ]}; // v V  Ì§  Ì¨
    key <AB05>	{[                b,              B,       dead_stroke,              none ]}; // b B  Ìµ
    key <AB06>	{[                n,              N,        dead_tilde,              none ]}; // n N  Ìƒ
    key <AB07>	{[           period,       question,      questiondown,              none ]}; // . ? Â¿
    key <AB08>	{[            comma,         exclam,        exclamdown,   dead_belowcomma ]}; // , ! Â¡  Ì¦
    key <AB09>	{[            colon,       ellipsis,    periodcentered,              none ]}; // : â€¦ Â·
    key <AB10>	{[        semicolon,          equal,      similarequal,          notequal ]}; // ; = â‰ƒ â‰ 

    include "nbsp(level3n)"
    include "keypad(oss)"

    include "level3(ralt_switch)"
};
