xkb_symbols "us" {

    include "us(basic)"
    name[Group1]= "French (US)";


    key <TLDE> { [     grave, asciitilde,    dead_grave                   ] };
    key <AE04> { [         4,     dollar,      EuroSign,         currency ] }; // â‚¬ Â¤
    key <AE06> { [         6,asciicircum,dead_circumflex                  ] };

    key <AD03> { [	   e,          E,        eacute,           Eacute ] }; // Ã© Ã‰
    key <AD04> { [	   r,          R,        egrave,           Egrave ] }; // Ã¨ Ãˆ
    key <AD06> { [	   y,          Y,    ydiaeresis,       Ydiaeresis ] }; // Ã¿ Å¸
    key <AD07> { [	   u,          U,        ugrave,           Ugrave ] }; // Ã¹ Ã™
    key <AD08> { [	   i,          I,    idiaeresis,       Idiaeresis ] }; // Ã¯ Ã
    key <AD09> { [	   o,          O,   ocircumflex,      Ocircumflex ] }; // Ã´ Ã”
    key <AD10> { [	   p,          P,            oe,               OE ] }; // Å“ Å’
    key <AD11> { [ bracketleft,  braceleft,  guillemotleft,  leftdoublequotemark ] }; // Â« â€œ
    key <AD12> { [bracketright, braceright, guillemotright, rightdoublequotemark ] }; // Â» â€

    key <AC01> { [	   a,          A,        agrave,           Agrave ] }; // Ã  Ã€
    key <AC02> { [	   s,          S,            ae,               AE ] }; // Ã¦ Ã†
    key <AC03> { [	   d,          D,   ecircumflex,      Ecircumflex ] }; // Ãª ÃŠ
    key <AC04> { [	   f,          F,    ediaeresis,       Ediaeresis ] }; // Ã« Ã‹
    key <AC06> { [	   h,          H,    udiaeresis,       Udiaeresis ] }; // Ã¼ Ãœ
    key <AC07> { [	   j,          J,   ucircumflex,      Ucircumflex ] }; // Ã» Ã›
    key <AC08> { [	   k,          K,   icircumflex,      Icircumflex ] }; // Ã® Ã
    key <AC11> { [apostrophe,   quotedbl,dead_diaeresis,           degree ] };

    key <AB01> { [	   z,          Z,   acircumflex,      Acircumflex ] }; // Ã¢ Ã‚
    key <AB03> { [	   c,          C,      ccedilla,         Ccedilla ] }; // Ã§ Ã‡

    include "eurosign(5)"
    include "level3(ralt_switch)"
};

