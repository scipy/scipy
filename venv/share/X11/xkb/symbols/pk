xkb_symbols "olpc" {

    // #HW-SPECIFIC

    // Contact: Walter Bender <walter@laptop.org>

    include "pk(ara)"

    name[Group1]= "Urdu (Pakistan)";

    key <TLDE>	{[  U0654,     U064B  ]};
