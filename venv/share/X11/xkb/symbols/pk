    key <AD07>	{[  U0621,     U0626  ]};
    key <AD08>	{[  U06CC,     U0650  ]};
    key <AD09>	{[  U06C1,     U06C3  ]};
    key <AD10>	{[  U067E,     U064F  ]};
    key <AD11>	{[  U005D,     U007D  ]};
    key <AD12>	{[  U005B,     U007B  ]};

    key <AC01>	{[  U0627,     U0622  ]};
    key <AC02>	{[  U0633,     U0635  ]};
