	key <AB05> { [	U0062,		U0042,		U201D,		U2019		] }; // b B ” ’
    // B row; Lower row, right side
	key <AB06> { [	U006B,		U004B,		VoidSymbol,	VoidSymbol	] }; // k K
	key <AB07> { [	U006D,		U004D,		U00B5,		U00BA		] }; // m M µ º
