//Name                  :       Sarala
//Description           :       This is an adaptation of the Sarala keyboard (http://www.medhajananam.org/sarala/) developed
//                              by Krishna Dhullipalla. Because of the way keyboard shortcuts are laid out in KDE, the keyboard
//                              modifiers had to be changed. The layout does not take any part of the original Sarala keyboard
//                              code however. It has been developed from scratch, so the experience may differ.
//
//                              There is a ibus-m17n version of Sarala layout developed by Satya Pothamsetti <potham@gmail.com> on
//                              http://www.medhajananam.org/.
//Standard		:	Supports Unicode 9.0.
//Help			:	This layout differs slightly from the layout on Medhajenanam. The layout has been depicted in the
//				pdf file attached to this post on Sarala google group.
//				(https://groups.google.com/forum/#!topic/sarala-keyboard/-gsa90dUFcs).
//
//Layout Developed by   :       Krishna Dhullipalla <krishnadvr@yahoo.com> (http://www.medhajananam.org/)
//Author                :       Venkat R Akkineni <venkatram.akkineni@india.com>
//Date			:	Apr 28 2017
partial alphanumeric_keys
xkb_symbols "tel-sarala"
{
    name[Group1] = "Telugu (Sarala)";
    key.type[group1]="FOUR_LEVEL";

    key <AB01> {[       U0C4A,       U0C12                           ]}; //   
    key <AB02> {[       U0C42,       U0C0A                           ]}; //  
    key <AB03> {[       U0C21,       U0C22                           ]}; //  
    key <AB04> {[       U0C35,       U0C39                           ]}; //  
    key <AB05> {[       U0C2C,       U0C2D                           ]}; //  
    key <AB06> {[       U0C28,       U0C23                           ]}; //  
    key <AB07> {[       U0C2E,       U0C01                           ]}; //  
    key <AB08> {[       comma,       U0C1E,         less             ]}; // ,  <
    key <AB09> {[      period,       U0C19,      greater             ]}; // .  >
    key <AB10> {[       U0C36,    question,        slash             ]}; //  ? /

    key <AC01> {[       U0C2F,       U0C3D                           ]}; //  
    key <AC02> {[       U0C02,       U0C03                           ]}; //  
    key <AC03> {[       U0C26,       U0C27                           ]}; //  
    key <AC04> {[       U0C4D,       U0C05                           ]}; //   
    key <AC05> {[       U0C17,       U0C18                           ]}; //  
    key <AC06> {[       U0C1A,       U0C1B,        U0C58,      U0C59 ]}; //    
    key <AC07> {[       U0C3E,       U0C06                           ]}; //   
    key <AC08> {[       U0C15,       U0C16,        U0C62,      U0C63 ]}; //    
    key <AC09> {[       U0C32,       U0C33,        U0C0C,      U0C61 ]}; //    
    key <AC10> {[       U0C1F,       U0C20,    semicolon,      colon ]}; //   ; :
    key <AC11> {[  apostrophe,    quotedbl                           ]}; // ' "

    key <AD01> {[       U0C46,       U0C0E,        U0C44,      U0C34 ]}; //     
    key <AD02> {[       U0C38,       U0C37,        U0C44             ]}; //   
    key <AD03> {[       U0C47,       U0C0F,        U0C44             ]}; //    
    key <AD04> {[       U0C30,       U0C31,        U0C44,      U0C60 ]}; //    
    key <AD05> {[       U0C24,       U0C25                           ]}; //  
    key <AD06> {[       U0C40,       U0C08                           ]}; //   
    key <AD07> {[       U0C41,       U0C09                           ]}; //  
    key <AD08> {[       U0C3F,       U0C07                           ]}; //   
    key <AD09> {[       U0C4B,       U0C13                           ]}; //   
    key <AD10> {[       U0C2A,       U0C2B                           ]}; //  
    key <AD11> {[       U0C1C,       U0C1D,  bracketleft,  braceleft ]}; //   [ {
    key <AD12> {[       U0C48,       U0C10, bracketright, braceright ]}; //    ] }

    key <AE01> {[           1,      exclam,        U0C67,      U0C78 ]}; // 1 !  
    key <AE02> {[           2,          at,        U0C68,      U0C79 ]}; // 2 @  
    key <AE03> {[           3,  numbersign,        U0C69,      U0C7A ]}; // 3 #  
    key <AE04> {[           4,      dollar,        U0C6A,      U0C7B ]}; // 4 $  
    key <AE05> {[           5,     percent,        U0C6B,      U0C7C ]}; // 5 %  
    key <AE06> {[           6, asciicircum,        U0C6C,      U0C7D ]}; // 6 ^  
    key <AE07> {[           7,   ampersand,        U0C6D,      U0C7E ]}; // 7 &  
    key <AE08> {[           8,    asterisk,        U0C6E,      U0C7F ]}; // 8 *  
    key <AE09> {[           9,   parenleft,        U0C6F,      U20B9 ]}; // 9 (  
    key <AE10> {[           0,  parenright,        U0C66,      U0C55 ]}; // 0 )  
    key <AE11> {[       minus,  underscore,     NoSymbol,      U0C56 ]}; // - _   
    key <AE12> {[       equal,        plus                           ]}; // = +
    key <BKSL> {[       U0C4C,       U0C14,        U0964,      U0965 ]}; //     
    key <TLDE> {[       U0C43,       U0C0B,        grave, asciitilde ]}; //   ` ~

    include "level3(ralt_switch)"
};

