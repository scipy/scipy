	xkb_symbols "geo" {
	include "it(geo)"
};

partial alphanumeric_keys
	xkb_symbols "olpc" {
	include "it(olpc)"
};

partial alphanumeric_keys
	xkb_symbols "us" {
	include "it(us)"
};

partial alphanumeric_keys
	xkb_symbols "ibm" {
	include "it(ibm)"
};
