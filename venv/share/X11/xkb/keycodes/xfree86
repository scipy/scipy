    <I59> =   217;
    <I5A> =   218;
    // <I5B> =   219;	<K74>
    // <I5C> =   220;	<K75>
    // <I5D> =   221;	<K76>
    <I5E> =   222;
    <I5F> =   223;
    <I60> =   224;
    <I61> =   225;
    <I62> =   226;
    <I63> =   227;
    <I64> =   228;
    <I65> =   229;
