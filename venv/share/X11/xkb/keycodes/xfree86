    <I30> =   176;
    <I31> =   177;
    <I32> =   178;
    <I33> =   179;
    <I34> =   180;
    // <I35> =   181;	<K5B>
    // <I36> =   182;	<K5D>
    // <I37> =   183;	<K5E>
    // <I38> =   184;	<K5F>
    <I39> =   185;
    <I3A> =   186;
    <I3B> =   187;
    <I3C> =   188;
    // <I3D> =   189;	<K62>
    // <I3E> =   190;	<K63>
    // <I3F> =   191;	<K64>
    // <I40> =   192;	<K65>
    // <I41> =   193;	<K66>
    <I42> =   194;
    <I43> =   195;
    <I44> =   196;
    <I45> =   197;
    // <I46> =   198;	<K67>
    // <I47> =   199;	<K68>
    // <I48> =   200;	<K69>
    // <I49> =   201;	<K6A>
    <I4A> =   202;
    // <I4B> =   203;	<K6B>
    // <I4C> =   204;	<K6C>
    // <I4D> =   205;	<K6D>
    // <I4E> =   206;	<K6E>
    // <I4F> =   207;	<K6F>
    // <I50> =   208;	<K70>
    // <I51> =   209;	<K71>
    // <I52> =   210;	<K72>
    // <I53> =   211;	<K73>
    <I54> =   212;
