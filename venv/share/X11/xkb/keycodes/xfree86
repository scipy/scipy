// "standard" XFree86 codes
// It seems that the "default" must be the first entry in the file.

default xkb_keycodes "xfree86" {
    include "xfree86(basic)"
    <BKSL> =  51;
    alias <AC12> = <BKSL>;
    <LSGT> =  94;
};

xkb_keycodes "basic" {

    minimum= 8;
    maximum= 255;

    <TLDE> =  49;
    alias <AE00> = <TLDE>;	// Some geometries use AE00
    <AE01> =  10;
    <AE02> =  11;
    <AE03> =  12;
    <AE04> =  13;
    <AE05> =  14;
    <AE06> =  15;
    <AE07> =  16;
