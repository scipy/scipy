\begin_inset Formula \[
\mu_{n}^{\prime}=\int_{0}^{1}\exp\left[-n\sigma\Phi^{-1}\left(y^{1/c}\right)\right]dy
\]
\end_inset

 
\begin_inset Formula \begin{eqnarray*}
\mu & = & \mu_{1}^{\prime}\\
\mu_{2} & = & \mu_{2}^{\prime}-\mu^{2}\\
\gamma_{1} & = & \frac{\mu_{3}^{\prime}-3\mu\mu_{2}-\mu^{3}}{\mu_{2}^{3/2}}\\
